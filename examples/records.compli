data rectangle = width: int, height: int
data scaled_rectangle = rect: rectangle, scale: float

func make_rectangle(): rectangle = rectangle(10, 20)
func make_scaled_rectangle(): scaled_rectangle = scaled_rectangle(make_rectangle(), 2.0)

func main(): int = (
	let rect = make_rectangle(),
		scaled_rect = make_scaled_rectangle(),
		foo = scaled_rectangle(rectangle(1, 2), 3.0),
		_ = trace(foo.scale),
		_ = trace(foo.rect.height)
	in
		rect.width
)
